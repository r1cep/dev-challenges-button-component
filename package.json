{
  "name": "dev-challenges-button-component",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "npm-run-all -p tsm:watch dev:vite",
    "dev:vite": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm-run-all -p lint:** tsc",
    "lint:eslint": "eslint '**/*.{js,jsx,ts,tsx,cjs,mjs,cts,mts}' --format friendly",
    "lint:stylelint": "stylelint '**/*.{css,scss}'",
    "lint:markuplint": "markuplint '**/*.tsx'",
    "lint:prettier": "prettier . --check",
    "fix": "npm-run-all -s fix:**",
    "fix:eslint": "eslint '**/*.{js,jsx,ts,tsx,cjs,mjs,cts,mts}' --fix",
    "fix:stylelint": "stylelint '**/*.{css,scss}' --fix",
    "fix:prettier": "prettier . --write",
    "tsc": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "tsm": "typed-scss-modules src/",
    "tsm:watch": "typed-scss-modules --watch src/",
    "storybook": "npm-run-all -p tsm:watch storybook:dev",
    "storybook:dev": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "build:storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "prepare": "husky install",
    "preinstall": "npx -y only-allow pnpm"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@markuplint/jsx-parser": "^2.4.2",
    "@markuplint/react-spec": "^2.1.1",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@tsconfig/strictest": "^1.0.1",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.23.4",
    "babel-loader": "^8.2.5",
    "destyle.css": "^3.0.2",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-testing-library": "^5.6.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-extended": "^3.1.0",
    "lint-staged": "^13.0.3",
    "markuplint": "^2.10.1",
    "node-sass": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-scss": "^4.0.5",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "require-from-string": "^2.0.2",
    "sass": "^1.55.0",
    "stylelint": "^14.12.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "typed-scss-modules": "^7.0.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vitest": "^0.23.4",
    "webpack": "^5.74.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
      }
    }
  },
  "engines": {
    "node": "^18.4.0",
    "pnpm": "^7.12.2"
  }
}
